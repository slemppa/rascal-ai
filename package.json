      },
      {
        "type": "docs",
        "section": "📚 Documentation"
      },
      {
        "type": "style",
        "section": "💄 Styles"
      },
      {
        "type": "refactor",
        "section": "♻️ Code Refactoring"
      },
      {
        "type": "perf",
        "section": "⚡ Performance Improvements"
      },
      {
        "type": "test",
        "section": "✅ Tests"
      },
      {
        "type": "build",
        "section": "📦 Build System"
      },
      {
        "type": "ci",
        "section": "👷 CI Configuration"
      },
      {
